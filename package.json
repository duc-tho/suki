        "client-serve": "webpack-dev-server --mode development --devtool inline-source-map --hot",
        "server": "nodemon -e ts --exec \"npm run compile && npm run start\"",
        "compile": "tsc -p ./src/server/tsconfig.json",
        "start": "node build/server",
        "build": "webpack --mode production && tsc -p ./src/server/tsconfig.json",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "dev-split": "concurrently \"npm run server\" \"npm run client-serve\"",
        "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'"
